{
  "ConnectionStrings": {
    "Default": "Server=localhost; Database=SampleDb; Trusted_Connection=True;"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "SoaClient": {
    "Address": [
      {
        "Ip": "127.0.0.1",
        "Port": "8007"
      },
      {
        "Ip": "127.0.0.1",
        "Port": "8008"
      }
    ],
    "Token": "token",
    "Transport": "Http",
    "AssemblyNames": "Soa.Sample.IService1,Soa.Sample.IService2",
    "HealthCheck": {
      "Enable": true,
      "JobCron": "0/5 * * * * ? ",
      "Timeout": 3000
    },
    "ServiceDiscovery": {
      "Enable": true,
      "JobCron": "0 * * * * ? "
    }

  }

}
